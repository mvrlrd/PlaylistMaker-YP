<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="312dp"
        android:height="312dp"
        android:gravity="center">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="?attr/bledniy_text_color"
                android:dashWidth="32dp"
                android:dashGap="32dp" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item
        android:width="@dimen/new_playlist_placeholder_image_size"
        android:height="@dimen/new_playlist_placeholder_image_size"
        android:drawable="@drawable/baseline_add_photo_alternate_24"
        android:gravity="center">
    </item>
</layer-list>
